<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<HBox prefHeight="449.0" prefWidth="1352.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.conflicts.blob.BlobConflictController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets right="5.0" />
         </HBox.margin>
         <children>
            <Label text="Ours:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextArea fx:id="textAreaOurs" editable="false" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets right="5.0" />
         </HBox.margin>
         <children>
            <Label text="Theirs:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextArea fx:id="textAreaTheirs" editable="false" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets right="5.0" />
         </HBox.margin>
         <children>
            <Label text="Ancestor:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextArea fx:id="textAreaAncestor" editable="false" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <Label text="Final Result:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextArea fx:id="textAreaFinalResult" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</HBox>
